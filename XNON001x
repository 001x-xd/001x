Tabs.Pet:AddSection("Pet")

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local petsService = ReplicatedStorage:WaitForChild("GameEvents"):WaitForChild("PetsService")

local toggleSwapLoop = false

Tabs.Pet:AddToggle("SwapPetLoadout", {
    Title = "PetLv",
    Default = false,
    Callback = function(state)
        toggleSwapLoop = state
        if state then
            task.spawn(function()
                while toggleSwapLoop do
                    -- ‡∏£‡∏±‡∏ô SwapPetLoadout 1
                    petsService:FireServer("SwapPetLoadout", 1)
                    task.wait(11) -- ‡∏£‡∏≠ 6 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ

                    -- ‡∏£‡∏±‡∏ô SwapPetLoadout 2
                    petsService:FireServer("SwapPetLoadout", 2)
                    task.wait(11) -- ‡∏£‡∏≠ 6 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡∏Å‡πà‡∏≠‡∏ô‡∏ß‡∏ô‡πÉ‡∏´‡∏°‡πà
                end
            end)
        end
    end
})


-- üêæ ‡∏õ‡∏∏‡πà‡∏°‡∏™‡πà‡∏á‡∏™‡∏±‡∏ï‡∏ß‡πå‡πÄ‡∏•‡∏µ‡πâ‡∏¢‡∏á‡∏ó‡∏µ‡πà‡∏ñ‡∏∑‡∏≠‡πÄ‡∏Ç‡πâ‡∏≤‡∏ï‡∏π‡πâ
Tabs.Pet:AddButton({
    Title = "SubmitHeldPet",
    Description = "‡∏™‡πà‡∏á‡∏™‡∏±‡∏ï‡∏ß‡πå‡πÄ‡∏•‡∏µ‡πâ‡∏¢‡∏á‡∏ó‡∏µ‡πà‡∏ñ‡∏∑‡∏≠‡πÄ‡∏Ç‡πâ‡∏≤‡∏ï‡∏π‡πâ",
    Callback = function()
        local args = {
            "SubmitHeldPet"
        }
        game:GetService("ReplicatedStorage")
            :WaitForChild("GameEvents")
            :WaitForChild("PetMutationMachineService_RE")
            :FireServer(unpack(args))
    end
})


Tabs.Pet:AddSection("Sell")

Tabs.Pet:AddToggle("AutoClaimReward", {
    Title = "Auto Sell Pet Held",
    Description = "‡∏Ç‡∏≤‡∏¢‡∏™‡∏±‡∏ï‡∏ß‡πå‡∏ó‡∏µ‡πà‡∏ñ‡∏∑‡∏≠‡∏≠‡∏¢‡∏π‡πà",
    Default = false,
    Callback = function(State)
        if State then
            -- ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Å‡∏î‡πÄ‡∏õ‡∏¥‡∏î Toggle
            task.spawn(function()
                while Toggles.AutoClaimReward.Value do
                    local args = {
                        Instance.new("Tool")
                    }

                    game:GetService("ReplicatedStorage")
                        :WaitForChild("GameEvents")
                        :WaitForChild("SellPet_RE")
                        :FireServer(unpack(args))

                    task.wait(1) -- ‡∏´‡∏ô‡πà‡∏ß‡∏á 1 ‡∏ß‡∏¥ ‡∏õ‡∏£‡∏±‡∏ö‡πÑ‡∏î‡πâ
                end
            end)
        end
        -- ‡∏ñ‡πâ‡∏≤‡∏õ‡∏¥‡∏î Toggle ‡∏ß‡∏ô while ‡∏à‡∏∞‡∏´‡∏•‡∏∏‡∏î‡∏≠‡∏≠‡∏Å‡πÄ‡∏≠‡∏á‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
    end
})



Tabs.Pet:AddButton({
    Title = "Sell All Pets", -- üîπ ‡∏ä‡∏∑‡πà‡∏≠‡∏õ‡∏∏‡πà‡∏° ‡∏†‡∏≤‡∏©‡∏≤‡∏≠‡∏±‡∏á‡∏Å‡∏§‡∏©
    Description = "‡∏Ç‡∏≤‡∏¢‡∏™‡∏±‡∏ï‡∏ß‡πå‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏•‡πá‡∏≠‡∏Ñ‡∏Å‡πá‡∏´‡∏≤‡∏¢", -- üîπ ‡∏Ñ‡∏≥‡∏≠‡∏ò‡∏¥‡∏ö‡∏≤‡∏¢ ‡∏†‡∏≤‡∏©‡∏≤‡πÑ‡∏ó‡∏¢
    Callback = function()
        game:GetService("ReplicatedStorage")
            :WaitForChild("GameEvents")
            :WaitForChild("SellAllPets_RE")
            :FireServer()
    end
})


Tabs.Main:AddSection("Trading")

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local SendTradeRemote = ReplicatedStorage:WaitForChild("GameEvents"):WaitForChild("TradeEvents"):WaitForChild("SendRequest")

-- ‡πÄ‡∏Å‡πá‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å
local selectedPlayer = nil

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏î‡∏∂‡∏á‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
local function getPlayerNames()
    local names = {}
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= Players.LocalPlayer then -- ‡πÑ‡∏°‡πà‡πÄ‡∏≠‡∏≤‡∏ï‡∏±‡∏ß‡πÄ‡∏≠‡∏á
            table.insert(names, plr.Name)
        end
    end
    return names
end

-- Dropdown ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
local playerDropdown = Tabs.Main:AddDropdown("PlayerSelect", {
    Title = "Player", -- üîπ ‡∏ä‡∏∑‡πà‡∏≠‡∏õ‡∏∏‡πà‡∏°‡∏†‡∏≤‡∏©‡∏≤‡∏≠‡∏±‡∏á‡∏Å‡∏§‡∏©
    Description = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
    Values = getPlayerNames(),
    Default = nil
})
playerDropdown:OnChanged(function(Value)
    selectedPlayer = Value
end)

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏Ñ‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤/‡∏≠‡∏≠‡∏Å
Players.PlayerAdded:Connect(function()
    playerDropdown:SetValues(getPlayerNames())
end)
Players.PlayerRemoving:Connect(function()
    playerDropdown:SetValues(getPlayerNames())
end)

-- ‡∏õ‡∏∏‡πà‡∏° Refresh Dropdown
Tabs.Main:AddButton({
    Title = "Refresh", -- üîπ ‡∏†‡∏≤‡∏©‡∏≤‡∏≠‡∏±‡∏á‡∏Å‡∏§‡∏©
    Description = "‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä", -- üîπ ‡∏†‡∏≤‡∏©‡∏≤‡πÑ‡∏ó‡∏¢
    Callback = function()
        playerDropdown:SetValues(getPlayerNames())
    end
})

-- ‡∏õ‡∏∏‡πà‡∏°‡∏™‡πà‡∏á‡∏ö‡∏±‡∏ï‡∏£‡πÄ‡∏ó‡∏£‡∏î
Tabs.Main:AddButton({
    Title = "Trade Ticket", -- üîπ ‡∏†‡∏≤‡∏©‡∏≤‡∏≠‡∏±‡∏á‡∏Å‡∏§‡∏©
    Description = "‡∏™‡πà‡∏á‡∏ö‡∏±‡∏ï‡∏£‡πÄ‡∏ó‡∏£‡∏î‡πÉ‡∏´‡πâ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å", -- üîπ ‡∏†‡∏≤‡∏©‡∏≤‡πÑ‡∏ó‡∏¢
    Callback = function()
        if selectedPlayer then
            local args = { Players:FindFirstChild(selectedPlayer) }
            if args[1] then
                SendTradeRemote:FireServer(unpack(args))
                print("‡∏™‡πà‡∏á‡∏Ñ‡∏≥‡∏Ç‡∏≠‡πÄ‡∏ó‡∏£‡∏î‡πÑ‡∏õ‡∏ó‡∏µ‡πà:", selectedPlayer)
            else
                warn("‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô:", selectedPlayer)
            end
        else
            warn("‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô")
        end
    end
})

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local PetGiftRemote = ReplicatedStorage:WaitForChild("GameEvents"):WaitForChild("PetGiftingService")

-- ‡πÄ‡∏Å‡πá‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å
local selectedPlayer = nil

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏î‡∏∂‡∏á‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
local function getPlayerNames()
    local names = {}
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= Players.LocalPlayer then -- ‡πÑ‡∏°‡πà‡πÄ‡∏≠‡∏≤‡∏ï‡∏±‡∏ß‡πÄ‡∏≠‡∏á
            table.insert(names, plr.Name)
        end
    end
    return names
end

-- Dropdown ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
local playerDropdown = Tabs.Main:AddDropdown("PlayerSelect", {
    Title = "Player",
    Description = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
    Values = getPlayerNames(),
    Default = nil
})
playerDropdown:OnChanged(function(Value)
    selectedPlayer = Value
end)

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏Ñ‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤/‡∏≠‡∏≠‡∏Å
Players.PlayerAdded:Connect(function()
    playerDropdown:SetValues(getPlayerNames())
end)
Players.PlayerRemoving:Connect(function()
    playerDropdown:SetValues(getPlayerNames())
end)

-- ‡∏õ‡∏∏‡πà‡∏° Refresh Dropdown
Tabs.Main:AddButton({
    Title = "Refresh",
    Description = "‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä",
    Callback = function()
        playerDropdown:SetValues(getPlayerNames())
    end
})

-- ‡∏õ‡∏∏‡πà‡∏°‡∏™‡πà‡∏á Pet Gift
Tabs.Main:AddButton({
    Title = "Trade Pet",
    Description = "‡∏™‡πà‡∏á‡∏™‡∏±‡∏ï‡∏ß‡πå‡πÄ‡∏•‡∏µ‡πâ‡∏¢‡∏á",
    Callback = function()
        if selectedPlayer then
            local target = Players:FindFirstChild(selectedPlayer)
            if target then
                local args = {
                    "GivePet", -- action
                    target     -- ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å
                }
                PetGiftRemote:FireServer(unpack(args))
                print("‡∏™‡πà‡∏á‡∏™‡∏±‡∏ï‡∏ß‡πå‡πÄ‡∏•‡∏µ‡πâ‡∏¢‡∏á‡πÑ‡∏õ‡∏ó‡∏µ‡πà:", selectedPlayer)
            else
                warn("‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô:", selectedPlayer)
            end
        else
            warn("‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô")
        end
    end
})




