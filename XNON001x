---------------------------------------------------------------------
-- Anti AFK (Toggle)
---------------------------------------------------------------------
do
    local antiAFKConn
    local antiAFKEverStarted = false -- ‡πÉ‡∏ä‡πâ‡∏ï‡∏£‡∏ß‡∏à‡∏ß‡πà‡∏≤‡∏°‡∏µ‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏¥‡∏î‡∏°‡∏≤‡∏Å‡πà‡∏≠‡∏ô‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà

    Tabs.Log:AddToggle("AntiAFKToggle", {
        Title = "Anti AFK",
        Default = false,
        Callback = function(state)
            if state then
                antiAFKEverStarted = true
                if antiAFKConn then antiAFKConn:Disconnect() end

                local VirtualUser = game:GetService("VirtualUser")
                antiAFKConn = LocalPlr.Idled:Connect(function()
                    VirtualUser:CaptureController()
                    VirtualUser:ClickButton2(Vector2.new())
                end)

                StarterGui:SetCore("SendNotification", {
                    Title = "XNON HUB",
                    Icon = "rbxassetid://79996080860226",
                    Text = "‡πÄ‡∏õ‡∏¥‡∏î‡∏Å‡∏±‡∏ô‡πÄ‡∏ï‡∏∞20‡∏ô.‡πÅ‡∏•‡πâ‡∏ß",
                    Duration = 5,
                })
            else
                if antiAFKConn then
                    antiAFKConn:Disconnect()
                    antiAFKConn = nil
                end

                -- ‡πÅ‡∏à‡πâ‡∏á‡∏õ‡∏¥‡∏î‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡∏ñ‡πâ‡∏≤‡πÄ‡∏Ñ‡∏¢‡πÄ‡∏õ‡∏¥‡∏î‡∏°‡∏≤‡∏Å‡πà‡∏≠‡∏ô
                if antiAFKEverStarted then
                    StarterGui:SetCore("SendNotification", {
                        Title = "XNON HUB",
                        Icon = "rbxassetid://79996080860226",
                        Text = "‡∏õ‡∏¥‡∏î‡∏Å‡∏±‡∏ô‡πÄ‡∏ï‡∏∞20‡∏ô.‡πÅ‡∏•‡πâ‡∏ß",
                        Duration = 5,
                    })
                end
            end
        end
    })
end


